      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>SIMICS_REV</name>
          <description>Used to specify which revision/tag to build.  By default, it will build &quot;default&quot;</description>
          <defaultValue>default</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>USE_SAST_CONTAINER_REV</name>
          <description>When checked, the job will use the revision specified in SAST_CONTAINER_REV to derive the revision for each subprroject.  When not checked, the build will be configured to use the revisions specified in all other *_REV parameters.</description>
          <defaultValue>true</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SAST_CONTAINER_REV</name>
          <description>Used to specify which revision/tag of the sast_product repository to use.  Only used when USE_SAST_CONTAINER_REV is checked.</description>
          <defaultValue>default</defaultValue>
        </hudson.model.StringParameterDefinition>
        <com.cwctravel.hudson.plugins.extended__choice__parameter.ExtendedChoiceParameterDefinition>
          <name>CONTAINER_OVERRIDES</name>
          <description>When USE_SAST_CONTAINER_REV is checked, this parameter allows specific revision to overriden in the container.  This supports the use case, &quot;I want put together a development build of SAST that is reconfigured to use a specific revision of a subcomponent that I&apos;ve modified.</description>
          <quoteValue>false</quoteValue>
          <type>PT_MULTI_SELECT</type>
          <value>SIM_COMMON_REV,SIM_INTERFACE_REV,SIM_REMOTE_REV,SIM_UI_REV,AST_REV,SIM_REV,FSW_CONTAINER_REV</value>
          <propertyFile></propertyFile>
          <propertyKey></propertyKey>
          <defaultValue></defaultValue>
          <defaultPropertyFile></defaultPropertyFile>
          <defaultPropertyKey></defaultPropertyKey>
        </com.cwctravel.hudson.plugins.extended__choice__parameter.ExtendedChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SIM_COMMON_REV</name>
          <description>Used to specify which revision/tag of the SimCommon repository to use.  By default, it will use&quot;default&quot;</description>
          <defaultValue>default</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SIM_INTERFACE_REV</name>
          <description>Used to specify which revision/tag of the SimExternalInterfaces repository to use.  By default, it will use&quot;default&quot;</description>
          <defaultValue>default</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SIM_REMOTE_REV</name>
          <description>Used to specify which revision/tag of the SimRemoteModels repository to use.  By default, it will use &quot;default&quot;</description>
          <defaultValue>default</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SIM_REV</name>
          <description>Used to specify which revision/tag of the SimRemoteModels repository to use.  By default, it will use &quot;default&quot;</description>
          <defaultValue>default</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SIM_UI_REV</name>
