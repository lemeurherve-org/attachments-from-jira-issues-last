    <hudson.tasks.Shell>
      <command>#!/bin/bash -ex

TIMESTAMP=$(date +%Y%m%d_%H%M)
GIT_ID=$(git rev-parse HEAD)
GIT_ID_SHORT=$(git rev-parse --short=7 HEAD)

is_match=$( echo &quot;$GIT_BRANCH&quot; | grep -i -e &quot;^[0-9]\+\.[0-9]\+$&quot; -e &quot;^master$&quot; | xargs echo &quot;feature_branch&quot; )

echo GIT_ID=${GIT_ID} &gt;&gt; env.props

if [[ &quot;$is_match&quot; == &quot;feature_branch&quot; ]]; then
	echo GIT_TAG=${GIT_BRANCH}.${GIT_ID_SHORT} &gt;&gt; env.props
    echo BUILDVERSION=${GIT_BRANCH,,}.${GIT_ID_SHORT} &gt;&gt; env.props
else
	echo GIT_TAG=${GIT_BRANCH}_${TIMESTAMP}_${GIT_ID_SHORT} &gt;&gt; env.props
    echo BUILDVERSION=${GIT_BRANCH,,}_${TIMESTAMP}_${GIT_ID_SHORT} &gt;&gt; env.props
fi

echo MAVEN_REPO_LOCAL=/tmp/m2_app_install_BRANCH_version_to_TARGET_$env &gt;&gt; env.props</command>
